<%= form_with(model: @business, url: business_path(@business), method: :patch) do |form| %>


  <%= form.label 'Business Name: ' %>
  <%= form.text_field :name %>

  <%= form.label 'Business Website: ' %>
  <%= form.text_field :website %>

  <%= form.label 'Business Profile Picture: ' %>
  <%= form.file_field :avatar %>

  <%= form.label 'Business Profile Banner: ' %>
  <%= form.file_field :banner %>

  <%= form.label 'Business Address: ' %>
  <%= form.text_field :address %>

  <%= form.label 'Business City: ' %>
  <%= form.text_field :city %>

  <%= form.label 'Business State: ' %>
  <%= form.text_field :state %>

  <%= form.label 'Business Zip: ' %>
  <%= form.text_field :zip %>

  <%= form.label 'Business Email: ' %>
  <%= form.email_field :email %>

  <%= form.label 'Business Phone: ' %>
  <%= form.telephone_field :phone %>

  <!-- Add fields for other attributes as needed -->

  <%= form.submit "Update Profile", class: "btn btn-primary" %>
<% end %>
